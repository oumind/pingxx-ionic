{
  "env": {
    "browser": true
  },
  "globals": {
    "ionic": true,
    "IonicModule": true,
    "angular": true,
    "jqLite": true,
    "forEach": true,
    "isDefined": true,
    "isNumber": true,
    "isString": true,
    "noop": true,
    "extend": true,
    "deprecated": true,
    "cordova": true,
    "DocumentTouch": true,
    "StatusBar": true
  },
  "rules": {
    "quotes": false,
    "no-underscore-dangle": false,
    "no-unused-expressions": false,
    "curly": [2, "multi-line"],
    "strict": false,
    "no-use-before-define": false,
    "eqeqeq": false,
    "new-cap": [2, {"capIsNew": false}],
    "dot-notation": [2, {"allowKeywords": false}],
    "no-console": false,
    "no-return-assign": false,
    "no-shadow": false,
    "comma-dangle": false,
    "camelcase": [2, {"properties": "never"}]
  }
}
